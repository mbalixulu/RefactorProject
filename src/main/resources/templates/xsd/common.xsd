<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
           xmlns:comm="http://ws.online.fnb.co.za/v1/common/"
           xmlns:restrictions="http://ws.online.fnb.co.za/v1/restrictions/"
           targetNamespace="http://ws.online.fnb.co.za/v1/common/" elementFormDefault="qualified"
           attributeFormDefault="qualified" vc:minVersion="1.1">
    <xs:import schemaLocation="restrictions.xsd" namespace="http://ws.online.fnb.co.za/v1/restrictions/"/>
    <xs:complexType name="symbol" abstract="true">
        <xs:attribute name="tooltip" type="restrictions:tooltip-limit"/>
        <xs:attribute name="label" type="restrictions:bifrost-string"/>
        <xs:attribute name="symbol-type" type="restrictions:bifrost-string" fixed=""/>
        <xs:attribute name="errorMessage" type="restrictions:bifrost-string"/>
        <xs:attribute name="required" type="xs:boolean" default="false"/>
    </xs:complexType>
    <xs:complexType name="subTabGroup">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="subTab" type="comm:subTab" minOccurs="0" maxOccurs="7"/>
                </xs:sequence>
                <xs:attribute name="subTabGroupHeading" type="restrictions:subTabGroupHeading-limit" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="subTab">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="id" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="subTabLabel" type="restrictions:subtab-label-limit" use="required"/>
                <xs:attribute name="url" type="xs:anyURI" use="required"/>
                <xs:attribute name="selected" type="xs:boolean" default="false"/>
                <xs:attribute name="notifications" type="xs:int"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="viewGroup">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="view" type="comm:view" minOccurs="2" maxOccurs="6"/>
                </xs:sequence>
                <xs:attribute name="align" type="restrictions:align" use="optional" default="left"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="view">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="id" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="url" type="xs:anyURI" use="required"/>
                <xs:attribute name="type" type="restrictions:view-restriction" use="required"/>
                <xs:attribute name="text" type="restrictions:views-text-limit" use="required"/>
                <xs:attribute name="width" type="restrictions:small-ints" default="2"/>
                <xs:attribute name="notifications" type="xs:int"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="formLayout">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="form" type="comm:form" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="footerElements" type="comm:footerElement" minOccurs="0" maxOccurs="5"/>
                    <xs:element name="techReference" type="restrictions:bifrost-string" minOccurs="0" maxOccurs="1"/>
                </xs:sequence>
                <xs:attribute name="template" type="restrictions:formLayout-templates" default="standard"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="form">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="sections" type="comm:section" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attribute name="action" type="xs:string"/>
                <xs:attribute name="name" type="restrictions:limitAlphanumericAndUnderscore"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="section">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="symbol" type="comm:symbol" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attribute name="id" type="restrictions:bifrost-string"/>
                <xs:attribute name="align" type="restrictions:align" use="optional" default="left"/>
                <xs:attribute name="width" type="restrictions:width-types" use="optional" default="half"/>
                <xs:attribute name="flexiWidth" type="restrictions:flexi-width-limit" use="optional" default="6"/>
                <xs:attribute name="contentAlign" type="restrictions:align" use="optional" default="left"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="flexiDashboardSection">
        <xs:complexContent>
            <xs:extension base="comm:section">
                <xs:sequence>
                    <xs:element name="flexiDashboardSubSection" type="comm:flexiDashboardSubSection" minOccurs="1"
                                maxOccurs="10"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="flexiDashboardSubSection">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="flexiDashboardCard" type="comm:flexiDashboardCard" minOccurs="0" maxOccurs="30"/>
                </xs:sequence>
                <xs:attribute name="id" type="restrictions:bifrost-string"/>
                <xs:attribute name="width" type="restrictions:flexiDashboardSubSection-widths" default="full"/>
                <xs:attribute name="height" type="restrictions:flexiDashboardSubSection-height" default="8"/>
                <xs:attribute name="maxCardsInRow" type="restrictions:flexiDashboardSubSection-maxCardsInRow"
                              default="1"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="flexiDashboardCard">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="flexiDashboardCardColumn" type="comm:flexiDashboardCardColumn" minOccurs="1"
                                maxOccurs="12"/>
                </xs:sequence>
                <xs:attribute name="id" type="restrictions:bifrost-string"/>
                <xs:attribute name="height" type="restrictions:flexiDashboardSubSection-height" default="8"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="flexiDashboardCardColumn">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="symbol" type="comm:symbol" minOccurs="0" maxOccurs="50"/>
                </xs:sequence>
                <xs:attribute name="id" type="restrictions:bifrost-string"/>
                <xs:attribute name="width" type="restrictions:flexi-width-limit" default="3"/>
                <xs:attribute name="backgroundColor" type="restrictions:background-colors" default="paper"/>
                <xs:attribute name="textColor" type="restrictions:text-colors" default="black"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="divContainer">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence minOccurs="0" maxOccurs="25">
                    <xs:element name="divElement" type="comm:symbol"/>
                </xs:sequence>
                <xs:attribute name="id" type="restrictions:bifrost-string"/>
                <xs:attribute name="hidden" use="optional" type="xs:boolean" default="false"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="boxContainer">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="boxSymbol" type="comm:symbol" minOccurs="0" maxOccurs="25"/>
                    <!--  Please don't make use of this collection, it has been deprecated  -->
                    <xs:element name="box" type="comm:box" minOccurs="0" maxOccurs="25"/>
                </xs:sequence>
                <xs:attribute name="id" type="restrictions:bifrost-string"/>
                <xs:attribute name="minHeight" type="restrictions:boxContainer-height" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="box">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence minOccurs="0" maxOccurs="50">
                    <xs:element name="boxSymbol" type="comm:symbol"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="boxSplit">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence minOccurs="0" maxOccurs="50">
                    <xs:element name="boxSplitSymbol" type="comm:symbol"/>
                </xs:sequence>
                <xs:attribute name="width" type="restrictions:boxSplitWidth" default="100"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="cardLayout">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="breadcrumb" type="comm:breadcrumb" minOccurs="0"/>
                    <xs:element name="cards" type="comm:card" maxOccurs="4"/>
                </xs:sequence>
                <xs:attribute name="heading" type="restrictions:bifrost-string"/>
                <xs:attribute name="headingSize" type="restrictions:heading-sizes" default="3"/>
                <xs:attribute name="headingStyle" type="restrictions:heading-styles" default="dark"/>
                <xs:attribute name="subHeading" type="restrictions:bifrost-string"/>
                <xs:attribute name="subHeadingSize" type="restrictions:heading-sizes" default="2"/>
                <xs:attribute name="subHeadingStyle" type="restrictions:heading-styles" default="dark"/>
                <xs:attribute name="backgroundImageSrc" type="xs:string"/>
                <!--  The ID of the card that the carousel must scroll to on load, if any  -->
                <xs:attribute name="selectedCardOnCarousel" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="dashboardLayout">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="widget" type="comm:widget" minOccurs="2" maxOccurs="6"/>
                </xs:sequence>
                <xs:attribute name="id" type="restrictions:bifrost-string"/>
                <xs:attribute name="heading" type="restrictions:bifrost-string"/>
                <xs:attribute name="headingColor" type="restrictions:text-colors" default="black"/>
                <xs:attribute name="dashboardTemplate" type="restrictions:dashboard-templates" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="widget">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="symbol" type="comm:symbol" minOccurs="1" maxOccurs="25"/>
                </xs:sequence>
                <xs:attribute name="id" type="restrictions:bifrost-string"/>
                <xs:attribute name="backgroundImageSrc" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="webContentRenderLayout">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="url" type="xs:anyURI" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="redirect">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="url" type="xs:anyURI" use="required"/>
                <xs:attribute name="target" type="restrictions:target" use="required"/>
                <xs:attribute name="raw" type="xs:boolean"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="card" abstract="true">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="id" type="xs:string" use="required"/>
                <xs:attribute name="align" type="restrictions:align" default="center"/>
                <xs:attribute name="heading" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="headingStyle" type="restrictions:heading-styles" use="required"/>
                <xs:attribute name="bgImageSrc" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="simple-card">
        <xs:complexContent>
            <xs:extension base="comm:card">
                <xs:sequence>
                    <xs:element name="text" type="comm:rich-text"/>
                    <xs:element name="button" type="comm:atom-button" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="subHeading" type="restrictions:bifrost-string"/>
                <xs:attribute name="subHeadingStyle" type="restrictions:heading-styles" default="dark"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="icon-card">
        <xs:complexContent>
            <xs:extension base="comm:card">
                <xs:sequence>
                    <xs:element name="icon" type="comm:icon-button" minOccurs="0" maxOccurs="4"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="complex-card">
        <xs:complexContent>
            <xs:extension base="comm:card">
                <xs:sequence>
                    <xs:element name="button" type="comm:atom-button"/>
                    <xs:element name="text" type="comm:rich-text"/>
                </xs:sequence>
                <xs:attribute name="subHeading" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="subHeadingStyle" type="restrictions:heading-styles" use="required"/>
                <xs:attribute name="topHeading" type="restrictions:bifrost-string"/>
                <xs:attribute name="topHeadingStyle" type="restrictions:heading-styles"/>
                <!--  If a fgImageSrc is specified, width is automatically assumed to be 6  -->
                <xs:attribute name="fgImageSrc" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="rich-text">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="value" type="restrictions:bifrost-string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- <xs:complexType name="markdown-text"> -->
    <!-- <xs:complexContent> -->
    <!-- <xs:extension base="comm:rich-text"/> -->
    <!-- </xs:complexContent> -->
    <!-- </xs:complexType> -->
    <xs:complexType name="html-text">
        <xs:complexContent>
            <xs:extension base="comm:rich-text"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="atom-button">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="id" type="xs:string" use="required"/>
                <xs:attribute name="url" type="xs:anyURI" use="required"/>
                <xs:attribute name="width" type="restrictions:small-ints" default="3"/>
                <xs:attribute name="style" type="restrictions:symbol-styles" default="light"/>
                <xs:attribute name="target" type="restrictions:target" default="main"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="icon-button">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="id" type="restrictions:bifrost-string"/>
                <xs:attribute name="icon-name" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="url" type="xs:anyURI" use="required"/>
                <xs:attribute name="target" type="restrictions:target" default="main"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="icon-buttonGroup">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="icon-button" type="comm:icon-button" maxOccurs="5"/>
                </xs:sequence>
                <xs:attribute name="align" type="restrictions:align" default="center"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="breadcrumb">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="hyperlinks" type="comm:hyperlink" maxOccurs="6"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="imageType" abstract="true">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="height" type="xs:integer" default="68"/>
                <xs:attribute name="width" type="xs:integer" default="68"/>
                <xs:attribute name="sizeMetric" type="restrictions:imgSizeMetric" default="px"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="chartDataSeriesType">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:choice>
                    <xs:element name="chartDataPoint" type="comm:x-and-y-datapoint" maxOccurs="unbounded"/>
                    <xs:element name="stackedColumnChartDataPoint" type="comm:stackedColumnChartDataPoint"
                                maxOccurs="unbounded"/>
                </xs:choice>
                <xs:attribute name="name" type="restrictions:bifrost-string" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="stackedColumnChartDataPoint">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="data" type="xs:double" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attribute name="stack" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="pointWidth" type="xs:positiveInteger" default="12"/>
                <xs:attribute name="linkedTo" type="restrictions:bifrost-string" default=":previous"/>
                <xs:attribute name="name" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="color" type="restrictions:colorPalette" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="stackedColumnChartLabel">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="left" type="xs:int" use="required"/>
                <xs:attribute name="top" type="xs:int" use="required"/>
                <xs:attribute name="sizeMetric" type="restrictions:imgSizeMetric" default="px"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="x-and-y-datapoint">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:attribute name="xAxisValue" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="yAxisValue" type="xs:double" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="deepLink">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="params" type="comm:params" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="renderURL" type="xs:anyURI" use="required"/>
                <xs:attribute name="sourceApplication" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="destinationApplication" type="restrictions:bifrost-string" use="required"/>
                <xs:attribute name="overrideBridgeKey" type="xs:boolean" default="false"/>
                <xs:attribute name="bridgeKey" type="restrictions:bifrost-string" default="bridge"/>
                <xs:attribute name="encrypted" type="xs:boolean" default="true"/>
                <xs:attribute name="echoContextCache" type="xs:boolean" default="false"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="text" abstract="true">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="value" type="restrictions:bifrost-string"/>
                </xs:sequence>
                <xs:attribute name="boldTableCell" type="xs:boolean" default="false"/>
                <xs:attribute name="align" type="restrictions:align"/>
                <xs:attribute name="color" type="restrictions:text-colors"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="simpleText">
        <xs:complexContent>
            <xs:extension base="comm:text"></xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="textHeading">
        <xs:complexContent>
            <xs:extension base="comm:text">
                <xs:attribute name="size" type="restrictions:heading-sizes" default="4"/>
                <xs:attribute name="mobiSize" type="restrictions:heading-sizes"/>
                <xs:attribute name="inline" type="xs:boolean"/>
                <xs:attribute name="marginBottom" type="xs:boolean" default="true"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="textReadout">
        <xs:complexContent>
            <xs:extension base="comm:text">
                <xs:attribute name="subHeading" type="restrictions:bifrost-string"/>
                <xs:attribute name="headingColor" type="restrictions:text-colors" default="black"/>
                <xs:attribute name="valueColor" type="restrictions:text-colors" default="black"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="textParagraph">
        <xs:complexContent>
            <xs:extension base="comm:text">
                <xs:attribute name="type" type="restrictions:textParagraph-types"/>
                <xs:attribute name="subHeading" type="restrictions:bifrost-string"/>
                <xs:attribute name="bold" type="xs:boolean" default="true"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="textList">
        <xs:complexContent>
            <xs:extension base="comm:text">
                <xs:sequence>
                    <xs:element name="textListItem" type="comm:textListItem" minOccurs="0" maxOccurs="50"/>
                </xs:sequence>
                <xs:attribute name="type" type="restrictions:textList-types" default="ul"/>
                <xs:attribute name="style" type="restrictions:textList-styles"/>
                <xs:attribute name="subHeading" type="restrictions:bifrost-string"/>
                <xs:attribute name="bold" type="xs:boolean" default="true"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="textListItem">
        <xs:complexContent>
            <xs:extension base="comm:text"></xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="rawHTML">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="html" type="restrictions:bifrost-string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="amount">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="value" type="xs:decimal"/>
                </xs:sequence>
                <xs:attribute name="name" type="restrictions:bifrost-string"/>
                <xs:attribute name="autocomplete" type="restrictions:onOff"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="input">
        <xs:complexContent>
            <xs:extension base="comm:symbol">
                <xs:sequence>
                    <xs:element name="value" type="restrictions:bifrost-string"/>
                    <xs:element name="inputItem" type="comm:inputItem" minOccurs="0" maxOccurs="50"/>
                </xs:sequence>
                <xs:attribute name="inputType" type="restrictions:input-types"/>
                <xs:attribute name="name" type="restrictions:bifrost-string"/>
                <xs:attribute name="maxlength" type="restrictions:input-length" default="50"/>
                <xs:attribute name="message" type="restrictions:bifrost-string"/>
                <xs:attribute name="placeHolder" type="restrictions:bifrost-string"/>
                <xs:attribute name="unCheckedValue" type="restrictions:bifrost-string"/>
                <xs:attribute name="selected" type="xs:boolean" default="false"/>
                <xs:attribute name="disabled" type="xs:boolean" default="false"/>
                <xs:attribute name="mapSearchPlaceholder" type="restrictions:bifrost-string"/>
                <xs:attribute name="autofocus" type="xs:boolean" default="false"/>
                <xs:attribute name="legend" type="restrictions:bifrost-string"/>
                <xs:attribute name="minVal" type="xs:int"/>
                <xs:attribute name="maxVal" type="xs:int"/>
                <xs:attribute name="rangeType" type="restrictions:range-types" default="single"/>
                <xs:attribute name="url" type="xs:anyURI" use="optional"/>
                <xs:attribute name="divContainerId" type="restrictions:bifrost-string" use="optional"/>
                <xs:attribute name="readonly" type="xs:boolean" default="false"/>
                <xs:attribute name="predictive" type="xs:boolean" default="false"/>
                <xs:attribute name="predictiveText" type="restrictions:bifrost-string" use="optional"/>
                <xs:attribute name="predictiveTextNav" type="restrictions:bifrost-string" use="optional"/>
