{
  "apiVersion": "route.openshift.io/v1",
  "kind": "Route",
  "metadata": {
    "labels": {
      "app": "mandates-resolutions-ui"
    },
    "name": "mandates-resolutions-ui"
  },
  "spec": {
    "port": {
      "targetPort": "8080-tcp"
    },
    "to": {
      "kind": "Service",
      "name": "mandates-resolutions-ui",
      "weight": 100
    },
    "tls": {
      "termination": "edge",
      "insecureEdgeTerminationPolicy": "Redirect"
    },
    "wildcardPolicy": "None"
  }
}