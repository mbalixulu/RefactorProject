{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "labels": {
      "app": "spring-boot-starter-tts"
    },
    "name": "spring-boot-starter-tts"
  },
  "spec": {
    "ports": [
      {
        "name": "8080-tcp",
        "port": 8080,
        "protocol": "TCP",
        "targetPort": 8080
      }
    ],
    "selector": {
      "app": "spring-boot-starter-tts",
      "deployment": "spring-boot-starter-tts"
    },
    "sessionAffinity": "None",
    "type": "ClusterIP"
  }
}