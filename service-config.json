    "labels": {
      "app": "mandates-resolutions-ui"
    },
    "name": "mandates-resolutions-ui"
  },
  "spec": {
    "ports": [
      {
        "name": "8080-tcp",
        "port": 8080,
        "protocol": "TCP",
        "targetPort": 8080
