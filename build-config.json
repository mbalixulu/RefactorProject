{
  "apiVersion": "v1",
  "kind": "BuildConfig",
  "metadata": {
    "labels": {
      "build": "spring-boot-starter-ocep",
      "app": "spring-boot-starter-ocep"
    },
    "name": "spring-boot-starter-ocep"
  },
  "spec": {
    "failedBuildsHistoryLimit": 5,
    "nodeSelector": null,
    "output": {
      "to": {
        "kind": "ImageStreamTag",
        "name": "spring-boot-starter-ocep:latest"
      }
    },
    "postCommit": {},
    "resources": {
      "limits": {
        "cpu": "200m",
        "memory": "256Mi"
      },
      "requests": {
        "cpu": "5m",
        "memory": "64Mi"
      }
    },
    "runPolicy": "Serial",
    "source": {
      "binary": {},
      "type": "Binary"
    },
    "strategy": {
      "dockerStrategy": {
        "from": {
          "kind": "ImageStreamTag",
          "name": "openjdk-runtime:17-hotspot"
        }
      },
      "type": "Docker"
    },
    "successfulBuildsHistoryLimit": 5
  }
}
